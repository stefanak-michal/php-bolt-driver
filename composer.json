{
    "name": "stefanak-michal/bolt",
    "description": "PHP library to provide connectivity to graph database over TCP socket with Bolt specification",
    "keywords": ["neo4j", "bolt", "socket", "database", "memgraph", "graph", "tcp", "driver"],
    "homepage": "https://github.com/stefanak-michal/php-bolt-driver",
    "type": "library",
    "readme": "README.md",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "support": {
        "issues": "https://github.com/stefanak-michal/php-bolt-driver/issues",
        "source": "https://github.com/stefanak-michal/php-bolt-driver",
		"docs": "https://www.neo4j.com/docs/bolt/current/"
    },
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/michalstefanak"
        }
    ],
    "authors": [
        {
            "name": "Michal Stefanak",
            "role": "Developer",
            "homepage": "https://www.linkedin.com/in/michalstefanak/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-sockets": "Needed when using Socket connection class",
        "ext-openssl": "Needed when using StreamSocket connection class with SSL"
    },
    "scripts": {
        "test": [
            "@putenv XDEBUG_MODE=debug",
            "Composer\\Config::disableProcessTimeout",
            "phpunit"
        ]
    }
}
